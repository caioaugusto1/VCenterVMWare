@model string

<input hidden id="pageValueIdentity" value="@Model" />

<div class="content-wrapper" style="min-height: 976.4px;">

    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>DataStore</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">DataStore</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <section class="content">
        <div class="container-fluid">
            <!-- Small boxes (Stat box) -->
            <div class="row">

                @if (Model == "History")
                {
                    <div class="col-md-3">
                        @await Html.PartialAsync("~/Views/Shared/_partial/_filter/_InputDateRange.cshtml")
                    </div>
                }

                <div class="col-md-4">
                    @await Html.PartialAsync("~/Views/Shared/_partial/_filter/_ServerDropBox.cshtml", new { servers = ViewBag.Servers })
                </div>

                <div style="margin-right:5px;">
                    @await Html.PartialAsync("~/Views/Shared/_partial/_filter/_SearchBtn.cshtml")
                </div>

                <div style="margin-right:5px;">
                    @await Html.PartialAsync("~/Views/Shared/_partial/_filter/_cleanBtn.cshtml")
                </div>

            </div>

        </div><!-- /.container-fluid -->
    </section>

    <section class="content">
        <div class="row" id="main-speed-div">

        </div>
    </section>
</div>


@await Html.PartialAsync("~/Views/Shared/_partial/_modalAlert.cshtml")

<script src="~/theme/plugins/jquery/jquery.js"></script>
<script src="~/lib/plotly-speed/plotly-latest.min.js"></script>
<script src="~/theme/plugins/moment/moment.min.js"></script>
<script src="~/theme/plugins/inputmask/min/jquery.inputmask.bundle.min.js"></script>
<script src="~/theme/plugins/daterangepicker/daterangepicker.js"></script>
<script src="~/theme/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>

<script src="~/js/Ultil/Util.js"></script>
<script src="~/js/DataStore/dataStore.js"></script>
