@model List<VMViewModel>

<div class="content-wrapper" style="min-height: 976.4px;">

    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Inventário</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Inventário</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    @await Html.PartialAsync("~/Views/VM/_partial/_IndexTop.cshtml", Model)

    <section class="content">
        <div class="row">
            <div class="col-12">

                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title"></h3>
                    </div>
                    <!-- /.card-header -->

                    <div id="div-table" class="card-body">
                        <table id="dataTable" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Nome</th>
                                    <th>Memória</th>
                                    <th>CPU</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                            <tbody id="tbody-tableInformation">

                            </tbody>
                        </table>

                    </div>

                </div>

            </div>
        </div>
    </section>
</div>

<input hidden id="pageValueIdentity" value="GetByAPI" />


<script src="~/theme/plugins/jquery/jquery.js"></script>

<script src="~/theme/plugins/moment/moment.min.js"></script>
<script src="~/theme/plugins/inputmask/min/jquery.inputmask.bundle.min.js"></script>
<script src="~/theme/plugins/daterangepicker/daterangepicker.js"></script>

<script src="~/js/Ultil/Util.js"></script>
<script src="~/js/VM/vm.js"></script>